<%= form_for @recipe do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.fields_for :ingredients do |ing_f| %>
  <%= ing_f.label :name %>
  <%= ing_f.text_field :name %>
  <%= ing_f.label :quantity %>
  <%= ing_f.text_field :quantity %>
<% end %>
<%= f.submit "Create New Recipe" %>
<% end %>
